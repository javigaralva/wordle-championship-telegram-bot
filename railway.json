{
    "$schema": "https://railway.app/railway.schema.json",
    "build": {
      "builder": "NIXPACKS"
    },
    "deploy": {
      "runtime": "UNSPECIFIED",
      "numReplicas": 1,
      "sleepApplication": false,
      "restartPolicyType": "ON_FAILURE",
      "restartPolicyMaxRetries": 10,
      "multiRegionConfig": {
        "us-west2": {
          "numReplicas": 1,
          "skipMetalMigration": true
        },
        "us-east4-eqdc4a": {
          "numReplicas": 1,
          "skipMetalMigration": true
        },
        "europe-west4-drams3a": {
          "numReplicas": 1,
          "skipMetalMigration": true
        },
        "asia-southeast1-eqsg3a": {
          "numReplicas": 1,
          "skipMetalMigration": true
        }
      }
    }
  }